  "author": "Kira272921",
  "bin": {
    "nextrate": "./bin/run"
  },
  "homepage": "https://github.com/Kira272921/nextrate",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Kira272921/nextrate",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/package.json"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "chalk": "4.1.2",
    "figchalk": "^1.0.3",
    "inquirer": "^8.2.2",
    "listr": "^0.14.3",
    "object-path": "^0.11.8",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.1",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/object-path": "^0.11.1",
    "@types/shelljs": "^0.8.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "oclif": {
    "bin": "nextrate",
    "dirname": "nextrate",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "watch": "tsc -b -w"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Kira272921/nextrate/issues",
  "keywords": [
    "oclif",
    "cli",
    "next.js",
    "automation",
    "snippets"
  ],
  "types": "dist/index.d.ts"
}
